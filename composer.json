{
    "name": "quatrecentquatre/craft-dummy-data",
    "description": "Replace private data with dummy content",
    "type": "craft-plugin",
    "license": "proprietary",
    "support": {
        "email": "production@quatrecentquatre.com"
    },
    "require": {
        "php": ">=7.3",
        "craftcms/cms": "^3.5.0",
        "fakerphp/faker": "1.20.0",
        "illuminate/collections" : "^8.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "quatrecentquatre\\dummydata\\": "src/"
        }
    },
    "extra": {
        "handle": "dummy-data",
        "name": "Dummy Data",
        "developer": "QuatreCentQuatre",
        "documentationUrl": "",
        "class": "quatrecentquatre\\dummydata\\DummyData"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.3"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

